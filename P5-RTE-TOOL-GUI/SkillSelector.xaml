<Window x:Class="P5_RTE_TOOL_GUI.SkillSelector"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:P5_RTE_TOOL_GUI"
        mc:Ignorable="d"
        Title="SkillSelector" Height="460" Width="300">
    <Grid>
        <TextBox x:Name="searchInput" HorizontalAlignment="Left" Height="23" Margin="10,41,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="272"/>
        <Button x:Name="searchButton" Content="Search" HorizontalAlignment="Left" Margin="10,69,0,0" VerticalAlignment="Top" Width="272" Height="30"/>
        <Label x:Name="label" Content="Search for a Skill" HorizontalAlignment="Left" Margin="7,15,0,0" VerticalAlignment="Top"/>
        <Button x:Name="confirmButton" Content="Select" HorizontalAlignment="Left" Margin="10,385,0,0" VerticalAlignment="Top" Width="272" Height="34"/>
        <ListBox x:Name="skillList" HorizontalAlignment="Left" Height="276" Margin="10,104,0,0" VerticalAlignment="Top" Width="272">
            <ListBoxItem Content="1"/>
        </ListBox>
    </Grid>
</Window>
